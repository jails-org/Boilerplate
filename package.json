{
  "name": "jails-boilerplate",
  "version": "1.0.0",
  "description": "Simplicity is the ultimate sophistication",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development ENV=dev webpack-dev-server --port 3000 --host 0.0.0.0",
    "build": "NODE_ENV=production webpack",
	"test:unit": "mocha --require @babel/register ./src/**/spec.js",
	"test:integration": "cypress open",
    "typecheck": "tsc --allowJs --checkJs --noEmit src/**/*.js",
    "server": "yarn build && cd dist && python -m SimpleHTTPServer 3000"
  },
  "keywords": [],
  "dependencies": {
    "axios": "^0.18.0",
    "jails-js": "^4.0.4",
    "jails.packages": "jails-org/packages#v1.7.0",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
	"@babel/register": "^7.0.0",
	"@babel/preset-react": "^7.9.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "core-js": "3",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "imagemin-webpack-plugin": "^2.4.0",
    "mini-css-extract-plugin": "^0.5.0",
	"mocha": "^5.2.0",
	"cypress": "^4.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "rupture": "^0.7.1",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svg-spritemap-webpack-plugin": "^3.2.0",
    "terser-webpack-plugin": "^1.2.2",
    "typescript": "^3.6.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
