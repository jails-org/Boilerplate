{
  "name": "ultimate-static-boilerplate",
  "version": "1.0.0",
  "description": "Simplicity is the ultimate sophistication",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development ENV=dev webpack-dev-server --port 3000 --host 0.0.0.0",
    "build": "NODE_ENV=production webpack",
    "test:unit": "mocha --require @babel/register ./**/spec.js",
    "test:integration": "cypress open",
    "typecheck": "tsc --allowJs --checkJs --noEmit ./**/*.js",
    "server": "yarn build && cd dist && python ../server.py --port=3000"
  },
  "keywords": [],
  "dependencies": {
    "axios": "^0.19.2",
    "bootswatch": "^4.5.0",
    "jails-js": "jails-org/jails",
    "jails.packages": "jails-org/packages"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.4",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "cypress": "^4.9.0",
    "file-loader": "^6.0.0",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "rupture": "^0.7.1",
    "style-loader": "^1.2.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "svg-spritemap-webpack-plugin": "^3.5.6",
    "terser-webpack-plugin": "^3.0.6",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^5.1.3"
  }
}
